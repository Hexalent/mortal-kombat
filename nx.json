{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "npmScope": "with-nx",
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "format",
          "typecheck",
          "lint",
          "test",
          "test:cov",
          "test:debug",
          "test:e2e",
          "build"
        ],
        "verbose": true,
        "silent": true
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "test:cov": {
      "outputs": [
        "{projectRoot}/coverage"
      ]
    }
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    }
  },
  "affected": {
    "defaultBase": "dev"
  }
}
